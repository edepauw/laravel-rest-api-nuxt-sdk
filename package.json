{
	"name": "lomkit-rest-client",
	"version": "1.0.5",
	"description": "TypeScript SDK for easily consuming lomkit/laravel-rest-api endpoints in Vue.js applications. Provides a typed, intuitive interface for handling resources.",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/types.d.mts",
			"import": "./dist/module.mjs"
		}
	},
	"main": "./dist/module.mjs",
	"typesVersions": {
		"*": {
			".": [
				"./dist/types.d.mts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"prepack": "nuxt-module-build build",
		"test": "vitest run",
		"test:ui": "vitest --ui --coverage.enabled=true",
		"release": "npm run test && npm run prepack && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edepauw/lomkit-rest-client.git"
	},
	"keywords": [
		"sdk",
		"laravel-rest-api",
		"laravel-client",
		"rest",
		"api",
		"nuxt"
	],
	"author": "Eliott Depauw",
	"bugs": {
		"url": "https://github.com/edepauw/lomkit-rest-client/issues"
	},
	"homepage": "https://github.com/edepauw/lomkit-rest-client#readme",
	"dependencies": {
		"@nuxt/kit": "^3.9.0"
	},
	"peerDependencies": {
		"nuxt": "^3.0.0"
	},
	"devDependencies": {
		"@nuxt/module-builder": "latest",
		"vitest": "^3.1.4",
		"@nuxt/test-utils": "^3.19.0",
		"@vitest/coverage-v8": "^3.1.4",
		"@vitest/ui": "^3.1.4"
	}
}
